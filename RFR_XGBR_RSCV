1.0],\n    'subsample': [0.6, 0.7, 0.8, 0.9, 1.0],\n    'gamma': [0, 0.1, 0.2, 0.3, 0.4]\n}\n\n#Creating instance of model\nxgb_model = XGBRegressor(objective='reg:squarederror', random_state=16, enable_categorical = True)\n\n#Use Randomized Search\nrandom_search = RandomizedSearchCV(\n    estimator=xgb_model,\n    param_distributions=param_distributions,\n    n_iter=50, # Number of parameter settings that are sampled\n    scoring='neg_mean_squared_error', \n    cv=5,\n    verbose=1,\n    n_jobs=-1, # Use all available cores\n    random_state=16,\n    return_train_score=True\n)\n\n# Fit the random search object to the training data\nrandom_search.fit(X_train, y_train)\n\nbest_model = random_search.best_estimator_\n\n# Log-space predictions\ny_pred_log = best_model.predict(X_valid)\n\n# Log-space RMSE\nrmse = np.sqrt(mean_squared_error(y_valid, y_pred_log))\n\nprint(\"Best parameters:\", random_search.best_params_)\nprint(\"CV RMSE:\", np.sqrt(-random_search.best_score_))\nprint(\"Validation RMSE:\", rmse)\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2026-01-22T04:04:11.967533Z","iopub.execute_input":"2026-01-22T04:04:11.967941Z","iopub.status.idle":"2026-01-22T04:04:38.399401Z","shell.execute_reply.started":"2026-01-22T04:04:11.967912Z","shell.execute_reply":"2026-01-22T04:04:38.398714Z"}},"outputs":[{"name":"stdout","text":"Fitting 5 folds for each of 50 candidates, totalling 250 fits\nBest parameters: {'subsample': 0.8, 'n_estimators': 200, 'max_depth': 3, 'learning_rate': 0.1, 'gamma': 0, 'colsample_bytree': 1.0}\nCV RMSE: 0.13781024831483768\nValidation RMSE: 0.13088474202955477\n","output_type":"stream"}],"execution_count":2},{"cell_type":"markdown","source":"# Tested Random Search","metadata":{}},{"cell_type":"code","source":"#Test Code\nrandom_search.fit(X,y)\n\nbest_model = random_search.best_estimator_\n\n#Define type of different columns if categorical\ntest_csv['HeatingQC'] = test_csv[\"HeatingQC\"].astype(\"category\")\ntest_csv[\"Kitc